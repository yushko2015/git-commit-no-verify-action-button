{
	"name": "git-commit-no-verify-action-button",
	"displayName": "git-commit-button",
	"description": "This exstension allow quickly commit changes without run pre-commit hooks",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-commit-no-verify-action-button.gitCommitNoVerify"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "git-commit-no-verify-action-button.gitCommitNoVerify",
				"title": "Commit (No verify)",
				"icon": {
					"light": "resources/git-commit-light.svg",
					"dark": "resources/git-commit-dark.svg"
				}
			}
		],
		"menus": {
			"scm/title": [
				{
					"when": "scmProvider == git",
					"command": "git-commit-no-verify-action-button.gitCommitNoVerify",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"ts-loader": "^9.2.6",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1",
		"@vscode/test-electron": "^2.0.3"
	}
}
